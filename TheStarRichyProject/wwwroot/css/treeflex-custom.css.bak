.tf-nc{
    border: none !important;
    max-width:50px;
    padding-right:0 !important;
    padding-left:0 !important;
}
  
.tf-tree li {
    align-items: center;
    display: flex;
    flex-direction: column;
    flex-wrap: wrap;
    padding: 0 0;
    position: relative;
}

.tf-tree li ul {
    margin-bottom: 1em;
}

.tf-custom .tf-nc:before,
.tf-custom .tf-nc:after {
    border-radius: 50px 20px;
}

.icon-w40 {
    width: 40px;
    height: 40px;
    margin: 2px;
}


.platinum-lv{
    content:url("../img/icons/tree-icon/1_Platinum_40x40.gif");
}

